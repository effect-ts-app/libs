  "exports": {
    "./_ext/*": {
      "import": {
        "types": "./_ext/*.d.ts",
        "default": "./_mjs/_ext/*.mjs"
      },
      "require": "./_ext/*.js"
    },
    "./polyfill": {
      "import": {
        "types": "./polyfill/*.d.ts",
        "default": "./_mjs/polyfill/*.mjs"
      },
      "require": "./polyfill/*.js"
    },
    "./polyfill/*": {
      "import": {
        "types": "./polyfill/*.d.ts",
        "default": "./_mjs/polyfill/*.mjs"
      },
      "require": "./polyfill/*.js"
    },
    "./types/*": "./types/*.ts"
  },
  "devDependencies": {
    "@effect-ts/core": "^0.60.2",
    "@effect-ts/fluent": "^0.3.13",
    "@effect-ts/fluent-node": "^0.1.3",
    "@effect-ts/monocle": "^0.43.0",
    "@effect-ts/node": "^0.39.0",
