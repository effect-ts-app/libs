    "@effect-ts/node": "^0.39.0",
    "@effect-ts/system": "^0.57.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && ttsc --build && yarn build2:mjs && yarn build2:cjs && sh ../../utils/rename.sh",
    "build2:cjs": "babel _esm --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .js --source-maps",
    "build2:mjs": "babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
    "watch": "ttsc --build --watch",
    "watch2": "ttsc -w",
    "prepare": "yarn build",
    "clean": "rm -rf ./_esm && rm -rf ./_mjs && rm -rf ./_cjs && find . | egrep '\\.m?js(\\.map)?$|\\.d.ts(\\.map)?$|\\.tsbuildinfo' | grep -v ./types | grep -v .eslint | grep -v jest.config | xargs rm -f",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
    "compile": "ttsc --noEmit",
    "lint": "eslint ./src ./types",
    "prettier": "prettier \"src/**/*.ts\" \"types/**/*.d.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "yarn lint && yarn test && yarn circular",
    "ncu": "ncu"
  },
  "gitHead": "ccd5a7c229c825a78737b4225f5f966d793a159c"
}
