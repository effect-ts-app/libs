{
    "name": "@effect-ts-app/react",
    "version": "0.23.0",
    "main": "./index.js",
    "license": "MIT",
    "dependencies": {
        "abort-controller": "^3.0.0",
        "cross-fetch": "^3.1.4",
        "path-parser": "^6.1.0"
    },
    "devDependencies": {
        "@effect-ts/core": "^0.40.8",
        "@effect-ts/monocle": "^0.34.0",
        "@effect-ts/schema": "^0.8.2",
        "@types/react": "^17.0.11",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "react": "^17.0.2"
    },
    "peerDependencies": {
        "@effect-ts/core": "^0.40.8",
        "@effect-ts/monocle": "^0.34.0",
        "@effect-ts/schema": "^0.8.2",
        "react": "^17.0.2"
    },
    "scripts": {
        "build": "yarn clean && tsc",
        "clean": "find . | egrep '\\.js(\\.map)?$|\\.d.ts$' | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false
}
