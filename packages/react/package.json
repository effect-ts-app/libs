{
    "name": "@effect-ts-app/react",
    "version": "0.57.0",
    "license": "MIT",
    "dependencies": {
        "abort-controller": "^3.0.0",
        "cross-fetch": "^3.1.4",
        "path-parser": "^6.1.0"
    },
    "devDependencies": {
        "@effect-ts/core": "^0.51.0",
        "@effect-ts/monocle": "^0.38.0",
        "@types/react": "^17.0.38",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "react": "^17.0.2"
    },
    "peerDependencies": {
        "@effect-ts/core": "^0.51.0",
        "@effect-ts/monocle": "^0.38.0",
        "react": "^17.0.2"
    },
    "scripts": {
        "build": "yarn clean && tsc --build && yarn build2:mjs && yarn build2:cjs && yarn generate-exports && sh ../../utils/rename.sh",
        "build2:cjs": "babel _esm --config-file ../../.babel.cjs.json --out-dir . --out-file-extension .js --source-maps",
        "build2:mjs": "babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
        "clean": "rm -rf ./_esm && find . | egrep '\\.m?js(\\.map)?$|\\.d.ts(\\.map)?$|\\.tsbuildinfo' | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "generate-exports": "sh ../../utils/extract.sh > package.log",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular",
        "ncu": "ncu"
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./_mjs/index.mjs",
            "require": "./index.js"
        },
        "./data": {
            "import": "./_mjs/data.mjs",
            "require": "./data.js"
        },
        "./hooks": {
            "import": "./_mjs/hooks.mjs",
            "require": "./hooks.js"
        },
        "./context": {
            "import": "./_mjs/context.mjs",
            "require": "./context.js"
        }
    },
    "sideEffects": false
}
