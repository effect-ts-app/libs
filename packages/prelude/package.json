{
    "name": "@effect-ts-app/prelude",
    "version": "0.75.0",
    "license": "MIT",
    "dependencies": {
        "@effect-ts-app/core": "^0.75.0",
        "@effect-ts-app/schema": "^0.75.0"
    },
    "devDependencies": {
        "@effect-ts/core": "^0.60.2",
        "@effect-ts/monocle": "^0.43.0"
    },
    "peerDependencies": {
        "@effect-ts/core": "^0.60.2",
        "@effect-ts/monocle": "^0.43.0"
    },
    "scripts": {
        "build": "yarn clean && ttsc --build && yarn build2:mjs && yarn build2:cjs && yarn generate-exports && sh ../../utils/rename.sh && cp -f ./index.js ./Prelude.js && cp -f ./_mjs/index.mjs ./_mjs/Prelude.mjs  && cp -f Prelude.d.ts index.d.ts",
        "build2:cjs": "babel _esm --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .js --source-maps",
        "build2:mjs": "babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
        "clean": "rm -rf ./_esm && rm -rf ./_mjs && rm -rf ./_cjs && find . | egrep '\\.m?js(\\.map)?$|\\.ts(\\.map)?$|\\.tsbuildinfo' | grep -v './index.ts' | grep -v /_ext/ | grep -v Prelude.d.ts | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "generate-exports": "sh ../../utils/extract.sh > package.log",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "ttsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular",
        "ncu": "ncu"
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./index.d.ts",
                "default": "./_mjs/index.mjs"
            },
            "require": "./index.js"
        },
        "./Prelude": {
            "import": {
                "types": "./Prelude.d.ts",
                "default": "./_mjs/Prelude.mjs"
            },
            "require": {
                "types": "./Prelude.d.ts",
                "default": "./Prelude.js"
            }
        },
        "./_ext/*": {
            "import": {
                "types": "./_ext/*.d.ts",
                "default": "./_mjs/_ext/*.mjs"
            },
            "require": "./_ext/*.js"
        },
        "./*.js": {
            "import": {
                "types": "./*.d.ts",
                "default": "./_mjs/*.mjs"
            },
            "require": "./*.js"
        }
    },
    "sideEffects": false,
    "gitHead": "ccd5a7c229c825a78737b4225f5f966d793a159c"
}
