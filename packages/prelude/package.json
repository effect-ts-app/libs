{
    "name": "@effect-ts-app/prelude",
    "version": "0.57.7",
    "license": "MIT",
    "dependencies": {
        "@effect-ts-app/core": "^0.57.7"
    },
    "devDependencies": {
        "@effect-ts/core": "^0.51.0",
        "@effect-ts/monocle": "^0.38.0"
    },
    "peerDependencies": {
        "@effect-ts/core": "^0.51.0",
        "@effect-ts/monocle": "^0.38.0"
    },
    "scripts": {
        "build": "yarn clean && tsc --build && yarn build2:mjs && yarn build2:cjs && yarn generate-exports && cp -f Prelude.d.ts index.d.ts",
        "build2:cjs": "babel _esm --config-file ../../.babel.cjs.json --out-dir . --out-file-extension .js --source-maps",
        "build2:mjs": "babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
        "clean": "rm -rf ./_esm && rm -rf ./_mjs && find . | egrep '\\.m?js(\\.map)?$|\\.d.ts(\\.map)?$|\\.tsbuildinfo' | grep -v Prelude.d.ts | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "generate-exports": "sh ../../utils/extract.sh > package.log",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular",
        "ncu": "ncu"
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./_mjs/index.mjs",
            "require": "./index.js"
        }
    },
    "sideEffects": false
}
